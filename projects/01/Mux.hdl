/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=b, b=sel, out=bANDsel);
    Not(in=sel, out=NOTsel);
    And(a=a, b=NOTsel, out=aNOTsel);
    Or(a=bANDsel, b=aNOTsel, out=out);  
}